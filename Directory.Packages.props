    </ItemGroup>
    <ItemGroup>
        <PackageVersion Include="ncrontab" Version="3.3.3"/>
        <PackageVersion Include="JetBrains.Annotations" Version="2025.2.2"/>
        <PackageVersion Include="System.Linq.Dynamic.Core" Version="1.6.7"/>
        <PackageVersion Include="YesSql.Abstractions" Version="5.4.6"/>
        <PackageVersion Include="Quartz" Version="3.15.0"/>
        <PackageVersion Include="Quartz.Extensions.Hosting" Version="3.15.0"/>
    </ItemGroup>
