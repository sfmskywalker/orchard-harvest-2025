        <PackageVersion Include="OrchardCore.Taxonomies.Core" Version="$(OrchardVersion)"/>
        <PackageVersion Include="OrchardCore.Title" Version="$(OrchardVersion)"/>
    </ItemGroup>
    <ItemGroup Label="Elsa.Extensions">
        <PackageVersion Include="Elsa.Agents.Activities" Version="3.6.0-preview.167"/>
        <PackageVersion Include="Elsa.Agents.Api" Version="$(ElsaExtensionsVersion)"/>
