        <PackageVersion Include="OrchardCore.Elsa.UI" Version="$(OrchardElsaVersion)"/>
    </ItemGroup>
    <ItemGroup Label="Orchard">
        <PackageVersion Include="OrchardCore.Admin.Abstractions" Version="$(OrchardVersion)"/>
        <PackageVersion Include="OrchardCore.Contents" Version="$(OrchardVersion)"/>
        <PackageVersion Include="OrchardCore.Infrastructure" Version="$(OrchardVersion)"/>
        <PackageVersion Include="OrchardCore.Module.Targets" Version="$(OrchardVersion)"/>
